    "# --- Teleportation circuit (with coherent corrections) ---\n",
    "def teleportation_circuit(theta=0.7*np.pi, phi=0.3*np.pi, lam=0.0, measure=True):\n",
    "    qc = QuantumCircuit(3, 2 if measure else 0, name=\"Teleportation_NoCond\")\n",
